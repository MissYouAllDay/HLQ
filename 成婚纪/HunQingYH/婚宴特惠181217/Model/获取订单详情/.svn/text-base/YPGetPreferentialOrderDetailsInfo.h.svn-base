#import <Foundation/Foundation.h>

NS_ASSUME_NONNULL_BEGIN

@interface YPGetPreferentialOrderDetailsInfo : NSObject

/**订单Id*/
@property (nonatomic, copy) NSString *Id;
/**新人名称*/
@property (nonatomic, copy) NSString *PeopleName;
/**新人手机号*/
@property (nonatomic, copy) NSString *Phone;
/**区域*/
@property (nonatomic, copy) NSString *Area;
/**商品名称*/
@property (nonatomic, copy) NSString *CommodityName;
/**服务商名称*/
@property (nonatomic, copy) NSString *HotelName;
/**服务时间*/
@property (nonatomic, copy) NSString *ServiceTime;
/**桌数*/
@property (nonatomic, copy) NSString *TableNumber;
/**餐标*/
@property (nonatomic, copy) NSString *MealAmount;
/**支付总额*/
@property (nonatomic, copy) NSString *TotalOrders;
/**立减金额*/
@property (nonatomic, copy) NSString *ReductionAmount;
/**支付金额*/
@property (nonatomic, copy) NSString *PaymentAmount;
/**支付尾款*/
@property (nonatomic, copy) NSString *PaidAmount;
/**服务费*/
@property (nonatomic, copy) NSString *ServiceCharge;
/**支付类型
 0待定,1定金,2结清,3全款*/
@property (nonatomic, copy) NSString *PaymentType;
/**承办婚礼状态
 0未承办,1已承办*/
@property (nonatomic, copy) NSString *UndertakeType;
/**定金*/
@property (nonatomic, copy) NSString *Earnestmoney;

/**商品封面图*/
@property (nonatomic, copy) NSString *CoverMap;
/**下单时间*/
@property (nonatomic, copy) NSString *CreateTime;
/**倒计时*/
@property (nonatomic, copy) NSString *ValidityTime;

@end

NS_ASSUME_NONNULL_END
