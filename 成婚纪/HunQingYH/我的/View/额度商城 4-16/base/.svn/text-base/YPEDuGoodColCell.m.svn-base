#import "YPEDuGoodColCell.h"

@implementation YPEDuGoodColCell

- (void)awakeFromNib {
    [super awakeFromNib];
    // Initialization code
    
//    self.layer.borderColor = CHJ_bgColor.CGColor;
//    self.layer.borderWidth = 1;
    self.backView.layer.shadowColor = GrayColor.CGColor;//shadowColor阴影颜色
    self.backView.layer.shadowOffset = CGSizeMake(1,1);//shadowOffset阴影偏移,x向右偏移4，y向下偏移4，默认(0, -3),这个跟shadowRadius配合使用
    self.backView.layer.shadowRadius = 2;
    self.backView.layer.shadowOpacity = 0.8;//阴影透明度，默认0
}

@end
